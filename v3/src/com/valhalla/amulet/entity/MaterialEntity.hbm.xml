<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.valhalla.amulet.entity.MaterialEntity" table="MATERIAL" schema="" catalog="athapong_amulet">
        <id name="materialCode">
            <column name="MATERIAL_CODE" sql-type="int" not-null="true"/>
        </id>
        <property name="materialDesc">
            <column name="MATERIAL_DESC" sql-type="varchar" length="45"/>
        </property>
    </class>
</hibernate-mapping>