<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.valhalla.amulet.entity.SearchStatisticEntity" table="SEARCH_STATISTIC" schema=""
           catalog="athapong_amulet">
        <id name="statNo">
            <column name="STAT_NO" sql-type="int" not-null="true"/>
            <generator class="increment"/>
        </id>
        <property name="memberId">
            <column name="MEMBER_ID" sql-type="int"/>
        </property>
        <property name="searchDate">
            <column name="SEARCH_DATE" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>