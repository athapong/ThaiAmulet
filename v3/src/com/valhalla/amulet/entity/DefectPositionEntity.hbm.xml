<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.valhalla.amulet.entity.DefectPositionEntity" table="DEFECT_POSITION" schema=""
           catalog="athapong_amulet">
        <id name="defectPosCode">
            <column name="DEFECT_POS_CODE" sql-type="int" not-null="true"/>
        </id>
        <property name="defectPostDesc">
            <column name="DEFECT_POST_DESC" sql-type="varchar" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>