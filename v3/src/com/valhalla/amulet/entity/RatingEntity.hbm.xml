<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.valhalla.amulet.entity.RatingEntity" table="RATING" schema="" catalog="athapong_amulet">
        <id name="ratingCode">
            <column name="RATING_CODE" sql-type="int" not-null="true"/>
        </id>
        <property name="ratingDesc">
            <column name="RATING_DESC" sql-type="varchar" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>